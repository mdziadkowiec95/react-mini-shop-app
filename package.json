{
  "name": "react-shop-app",
  "version": "1.0.0",
  "description": "MERN stack Shopping App",
  "main": "server.js",
  "scripts": {
    "client-install": "npm install --prefix client",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mdziadkowiec95/react-shop-app.git"
  },
  "author": "M. Dziadkowiec",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mdziadkowiec95/react-shop-app/issues"
  },
  "homepage": "https://github.com/mdziadkowiec95/react-shop-app#readme",
  "dependencies": {
    "body-parser": "^1.19.0",
    "concurrently": "^4.1.1",
    "connect-busboy": "0.0.2",
    "dotenv": "^8.0.0",
    "eslint-plugin-import": "^2.18.0",
    "express": "^4.17.1",
    "mongoose": "^5.6.2",
    "multer": "^1.4.1"
  },
  "devDependencies": {
    "nodemon": "^1.19.1"
  },
  "engines": {
    "node": "10.16.0"
  }
}